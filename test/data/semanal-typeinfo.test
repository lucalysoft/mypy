[case testEmptyFile]
[out]
TypeInfoMap()

[case testEmptyClass]
class c: pass
[out]
TypeInfoMap(
  __main__.c : TypeInfo(
    Name(__main__.c)
    Base(builtins.object)
    Names()))

[case testClassWithMethod]
class c:
  def f(self): pass
[out]
TypeInfoMap(
  __main__.c : TypeInfo(
    Name(__main__.c)
    Base(builtins.object)
    Names(
      f)))

[case testClassWithAttributes]
class c:
  def __init__(self, x):
    self.y = x
    self.z = 1
[out]
TypeInfoMap(
  __main__.c : TypeInfo(
    Name(__main__.c)
    Base(builtins.object)
    Names(
      __init__
      y
      z)))

[case testBaseClass]
class base: pass
class c(base): pass
[out]
TypeInfoMap(
  __main__.base : TypeInfo(
    Name(__main__.base)
    Base(builtins.object)
    Names())
  __main__.c : TypeInfo(
    Name(__main__.c)
    Base(__main__.base)
    Names()))

[case testClassAndAbstractClass]
from abc import abstractmethod, ABCMeta
import typing

class i(metaclass=ABCMeta): pass
class c(i): pass
[out]
TypeInfoMap(
  __main__.c : TypeInfo(
    Name(__main__.c)
    Base(__main__.i)
    Names())
  __main__.i : TypeInfo(
    Name(__main__.i)
    Base(builtins.object)
    Names()))

[case testAttributeWithoutType]
class A:
    a = A
[out]
TypeInfoMap(
  __main__.A : TypeInfo(
    Name(__main__.A)
    Base(builtins.object)
    Names(
      a)))
