-- Overloads + varargs
-- -------------------


[case testIntersectionTypesAndVarArgs]
from typing import Undefined, overload
a, b = Undefined, Undefined # type: (A, B)

b = f()        # E: Incompatible types in assignment
b = f(a)       # E: Incompatible types in assignment
b = f(a, b)    # E: Incompatible types in assignment
a = f(b)       # E: Incompatible types in assignment
a = f(b, b)    # E: Incompatible types in assignment
b = f(a, *[b]) # E: Incompatible types in assignment
b = f(*())
b = f(*(a,))   # E: Incompatible types in assignment
b = f(*(a, b)) # E: Incompatible types in assignment
a = f(*(b,))   # E: Incompatible types in assignment
a = f(*(b, b)) # E: Incompatible types in assignment
a = f(*[b])    # E: Incompatible types in assignment

a = f()
a = f(a)
a = f(a, b)
b = f(b)
b = f(b, b)
a = f(a, *[b])
a = f(*())
a = f(*(a,))
a = f(*(a, b))
b = f(*(b,))
b = f(*(b, b))
b = f(*[b])

@overload
def f(a: 'A' = None, *b: 'B') -> 'A': pass

@overload
def f(a: 'B', *b: 'B') -> 'B': pass

class A: pass
class B: pass
[builtins fixtures/list.py]


