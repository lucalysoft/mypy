-- Test cases for the type checker related to varargs.


-- Varargs within body
-- -------------------


[case testVarArgsWithinFunction]
from typing import Undefined, List
def f( *b: 'B') -> None:
    ab = Undefined # type: List[B]
    ac = Undefined # type: List[C]
    b = ac # E: Incompatible types in assignment
    ac = b # E: Incompatible types in assignment
    b = ab
    ab = b

class B: pass
class C: pass
[builtins fixtures/list.py]
[out]
main: In function "f":


