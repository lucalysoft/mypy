[out]
def f(x, **y): pass

[case testClass]
class A:
    def f(self, x):
        x = 1
def g(): pass
[out]
class A:
    def f(self, x): pass
def g(): pass

[case testVariable]
x = 1
[out]
from typing import Undefined, Any

x = Undefined(Any)

[case testClassVariable]
class C:
    x = 1
[out]
from typing import Undefined, Any

class C:
    x = Undefined(Any)

[case testSelfAssignment]
class C:
    def __init__(self):
        self.x = 1
        x.y = 2
[out]
from typing import Undefined, Any

class C:
    x = Undefined(Any)
    def __init__(self): pass

[case testSelfAndClassBodyAssignment]
x = 1
class C:
    x = 1
    def __init__(self):
        self.x = 1
        self.x = 1
[out]
from typing import Undefined, Any

x = Undefined(Any)
class C:
    x = Undefined(Any)
    def __init__(self): pass

[case testEmptyClass]
