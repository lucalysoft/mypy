(3, 1)   {n, r0, r3}             {n, r0, r3}

[case testMultiPass_Liveness]
def f(n: int) -> None:
    x = 1
    y = 1
    while n < 1:
        n = y
        while n < 2:
            n = 1
            n = x
[out]
L0:
    r0 = 1
    x = r0
    r1 = 1
    y = r1
L1:
    r2 = 1
    if n < r2 goto L2 else goto L6 :: int
L2:
    n = y
L3:
    r3 = 2
    if n < r3 goto L4 else goto L5 :: int
L4:
    r4 = 1
    n = r4
    n = x
    goto L3
L5:
    goto L1
L6:
    r5 = None
    return r5

