[case testSimple_Liveness]
def f(a: int) -> int:
    x = 1
    if x == 1:
        return a
    else:
        return x
[out]
L0:
    r0 = 1
    x = r0
    r1 = 1
    if x == r1 goto L1 else goto L2 :: int
L1:
    return a
L2:
    return x
L3:
