[case testLoop_MustDefined]
def f(n: int) -> None:
    while n < 5:
        n = n + 1
        m = n
[out]
L0:
L1:
    r0 = 5
    if n < r0 goto L2 else goto L3 :: int
L2:
    r1 = 1
    r2 = n + r1 :: int
    n = r2
    m = n
    goto L1
L3:
    r3 = None
    return r3

