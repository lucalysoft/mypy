[case testLoop_BorrowedArgument]
def f(a: int) -> int:
    sum = 0
    i = 0
    while i <= a:
        sum = sum + i
        i = i + 1
    return sum
[out]
L0:
    r0 = 0
    sum = r0
    r1 = 0
    i = r1
L1:
    if i <= a goto L2 else goto L3 :: int
L2:
    r2 = sum + i :: int
    sum = r2
    r3 = 1
    r4 = i + r3 :: int
    i = r4
    goto L1
L3:
    return sum

